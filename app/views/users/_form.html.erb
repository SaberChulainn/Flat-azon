<%= form_for user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %><br>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %><br>

    <%= f.label :email %>
    <%= f.text_field :email %><br>

    <%= f.label :address %>
    <%= f.text_area :address %><br>

    <%= f.label :profile_pic %>
    <%= f.text_field :profile_pic %><br>

    <%= f.submit %>
<% end %>